name: UL5
prefix: ""
topology:
  kinds:
    ceos:
      image: ceosimage:4.33.2F
      #startup-config: ./configs/default.cfg
  nodes:
    SW1:
      kind: ceos
      ports:
        - "2201:22"
      #startup-config: ./configs/SW1.cfg
    SW2:
      kind: ceos
      ports:
        - "2202:22"
      #startup-config: ./configs/SW2.cfg
    SW3:
      kind: ceos
      ports:
        - "2203:22"
      #startup-config: ./configs/SW3.cfg  
    R1:
      kind: ceos
      ports:
        - "2204:22"
      #startup-config: ./configs/R1.cfg  
    Controller:
      kind: ceos
      ports:
        - "2205:22"
      #startup-config: ./configs/Controller.cfg
  links:
    - endpoints:
        - R1:eth1
        - SW1:eth1
    - endpoints:
        - R1:eth2
        - SW2:eth1
    - endpoints:
        - R1:eth3
        - SW3:eth1
    - endpoints:
        - Controller:eth1
        - R1:eth4        
mgmt:
  network: clab
  ipv4-subnet: 172.100.100.0/24
